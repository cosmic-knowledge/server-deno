{
	"extends": ["@tsconfig/node20/tsconfig.json"],
	"include": ["server.ts", "init.ts", "node"],
	"compilerOptions": {
		"lib": ["ES2023", "DOM"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "es2022",
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": false
	}
}
